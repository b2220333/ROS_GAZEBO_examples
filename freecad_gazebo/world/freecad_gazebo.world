<?xml version="1.0" ?>

<!-- world檔由sdf檔所定義 -->
<sdf version="1.4">

  <world name="default">
  
    <!-- 地板-->  
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- 光線-->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- 定義視角從上往下，pose是 x y z roll pitch yaw -->
    
    <gui fullscreen='0'>
      <camera name='user_camera'>
        <pose>0 0 7 0 1.570796 1.570796</pose>
        <view_controller>orbit</view_controller>
      </camera>
    </gui>        

    <model name="carcar">
      <link name="body_link">
        <pose>0 0 0 0 0 0</pose>
        <visual name="body_visual">
          <geometry>
            <mesh><uri>file:///home/jaqq/ROS_GAZEBO_examples/freecad_gazebo/mesh/car_body.stl</uri><scale>0.001 0.001 0.001</scale></mesh>
          </geometry>
        </visual>
        <collision name="body_collision">
          <geometry>
            <mesh><uri>file:///home/jaqq/ROS_GAZEBO_examples/freecad_gazebo/mesh/car_body.stl</uri><scale>0.001 0.001 0.001</scale></mesh>
          </geometry>
        </collision>       
        
        <inertial>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
          <mass>10</mass>
        </inertial>
        
      </link>
      
      <link name="left_wheel_link">
        <pose>0.03 0.016 0 0 1.57079632679 1.57079632679</pose>
        <visual name="left_wheel_visual">          
          <geometry>
            <cylinder>
              <radius>0.01</radius>
              <length>0.01</length>
            </cylinder>
          </geometry>
        </visual>
        <collision name="left_wheel_collision">          
          <geometry>
            <cylinder>
              <radius>0.01</radius>
              <length>0.01</length>
            </cylinder>
          </geometry>
        </collision> 
        
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
          </friction>
        </surface>
        
        <inertial>
          <inertia>
            <ixx>0</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>0</izz>
          </inertia>
          <mass>10</mass>
        </inertial>
      </link>
      
      <link name="right_wheel_link">
        <pose>0.03 -0.016 0 0 1.57079632679 1.57079632679</pose>
        <visual name="right_wheel_visual">
          <geometry>
            <cylinder>
              <radius>0.01</radius>
              <length>0.01</length>
            </cylinder>
          </geometry>
        </visual>
        <collision name="right_wheel_collision">          
          <geometry>
            <cylinder>
              <radius>0.01</radius>
              <length>0.01</length>
            </cylinder>
          </geometry>
        </collision> 
        
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
          </friction>
        </surface>
        
        <inertial>
          <inertia>
            <ixx>0</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>0</izz>
          </inertia>
          <mass>10</mass>
        </inertial>
      </link>
      
      <link name="left_wheel_back_link">
        <pose>-0.02 0.016 0 0 1.57079632679 1.57079632679</pose>
        <visual name="left_wheel_back_visual">          
          <geometry>
            <cylinder>
              <radius>0.01</radius>
              <length>0.01</length>
            </cylinder>
          </geometry>
        </visual>
        <collision name="left_wheel_back_collision">          
          <geometry>
            <cylinder>
              <radius>0.01</radius>
              <length>0.01</length>
            </cylinder>
          </geometry>
        </collision> 
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
          </friction>
        </surface>
        
        <inertial>
          <inertia>
            <ixx>0</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>0</izz>
          </inertia>
          <mass>10</mass>
        </inertial>
      </link>
      
      <link name="right_wheel_back_link">
        <pose>-0.02 -0.016 0 0 1.57079632679 1.57079632679</pose>
        <visual name="right_wheel_back_visual">
          <geometry>
            <cylinder>
              <radius>0.01</radius>
              <length>0.01</length>
            </cylinder>
          </geometry>
        </visual>
        <collision name="right_wheel_back_collision">          
          <geometry>
            <cylinder>
              <radius>0.01</radius>
              <length>0.01</length>
            </cylinder>
          </geometry>
        </collision> 
        
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
          </friction>
        </surface>
        
        <inertial>
          <inertia>
            <ixx>0</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>0</izz>
          </inertia>
          <mass>10</mass>
        </inertial>
      </link>
      
      <joint name="robot_car_left_wheel_joint" type="revolute">
        <pose>0 0 0 0 0 0</pose>
        <axis>
          <xyz>0 1 0</xyz>        
        </axis>        
        <parent>body_link</parent>
        <child>left_wheel_link</child>
      </joint>
      
      <joint name="robot_car_right_wheel_joint" type="revolute">
        <pose>0 0 0 0 0 0</pose>
        <axis>
          <xyz>0 1 0</xyz>        
        </axis>        
        <parent>body_link</parent>
        <child>right_wheel_link</child>
      </joint>      
      
      <joint name="robot_car_left_wheel_back_joint" type="revolute">
        <pose>0 0 0 0 0 0</pose>
        <axis>
          <xyz>0 1 0</xyz>        
        </axis>        
        <parent>body_link</parent>
        <child>left_wheel_back_link</child>
      </joint>
      
      <joint name="robot_car_right_wheel_back_joint" type="revolute">
        <pose>0 0 0 0 0 0</pose>
        <axis>
          <xyz>0 1 0</xyz>        
        </axis>        
        <parent>body_link</parent>
        <child>right_wheel_back_link</child>
      </joint> 
      
    </model>
    
  </world>
</sdf>
